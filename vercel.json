{
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "/api/$1.js"
    },
    {
      "src": "/checkout.html",
      "dest": "/dev/checkout.html"
    },
    {
      "src": "/cart.html",
      "dest": "/dev/cart.html"
    },
    {
      "src": "/checkout",
      "dest": "/dev/checkout.html"
    },
	{
      "src": "/checkout2.html",
      "dest": "/dev/checkout2.html"
    }
    {
      "src": "/cart",
      "dest": "/dev/cart.html"
    },
    {
      "src": "/",
      "dest": "/dev/index.html"
    }
  ]
}
